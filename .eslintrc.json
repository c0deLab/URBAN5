{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "parser": "babel-eslint",
    "rules": {
      "comma-dangle": [2, "only-multiline"],
      "arrow-parens": [2, "as-needed"],
      "no-underscore-dangle": "off",
      "no-shadow": "off",
      "object-curly-newline": "off",
      "prefer-template": "off",
      "max-len": "off",
      "react/no-string-refs": "off",
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}
